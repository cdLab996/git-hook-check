{
  "name": "git-hook-check",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "check-keyword": "bash script/check-keyword.sh",
    "check-commit": "npm run check-keyword",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "pre-commit": "^1.2.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  },
  "pre-commit": [
    "check-commit"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "npm run check-commit",
      "git add"
    ]
  }
}
